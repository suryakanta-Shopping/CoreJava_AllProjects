package com.java.dowhileloop.statement;

public class DoWhile_Loop_Statement_02 {

	public static void main(String[] args) {

		//while loop
		
		int i = 10;
		while(i<9) {
			System.out.println("Hi");
		}
		
		//do while loop
		
		int j=11;
		
		do {
			System.out.println("HI");
		}
		while(j<9);
	}

}
